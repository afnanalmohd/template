<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/blankFragment">

    <fragment
        android:id="@+id/loginFragment"
        android:name="com.example.template.common.presentation.ui.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_profilrFragment"
            app:destination="@id/profilrFragment" />
    </fragment>
    <fragment
        android:id="@+id/profilrFragment"
        android:name="com.example.template.common.presentation.ui.ProfileFragment"
        android:label="fragment_profilr"
        tools:layout="@layout/fragment_profile" />

    <fragment
        android:id="@+id/blankFragment"
        android:name="com.example.template.BlankFragment"
        android:label="fragment_blank"
        tools:layout="@layout/fragment_blank" >
        <action
            android:id="@+id/action_blankFragment_to_profilrFragment"
            app:destination="@id/profilrFragment" />
    </fragment>
</navigation>